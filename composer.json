    "type": "sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Plisio\\PlisioSdkLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "plisdeveloper",
            "email": "plugins@plisio.net"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
